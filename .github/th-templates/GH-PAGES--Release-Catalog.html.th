        <div class="row">
        <h3>List of All Released Versions</h3>

          <div class="current-snapshot">
            <div class="snapshot-group-header">
              <span class="major-group-date-time" th:text="${git.releaseCatalog.snapshotGroup.finalDateTimeStr()}">
                current-snapshot-group-date-time
              </span>
              <span class="snapshot-group-version">
                [(${ gbh.badgeSnapshotJavaDocHTML })]
              </span>
              <span class="snapshot-group-info">
                [(${ gbh.badgeSnapshotGitHubHTML })]
                [[${ git.releaseCatalog.snapshotGroup.groupInfo }]]
              </span>
            </div>
            <div class="snapshot-group-commits" th:each="commitInfo: ${git.releaseCatalog.snapshotGroup.commits}">
              <span class="commit-date-time" th:text="${commitInfo.localCommitTimeStr()}">
                COMMIT-TIME-#0.1
              </span>
              <span class="commit-ID">
                [(${ gbh.badgeCommit(commitInfo) })]
              </span>
              <span class="commit-message" th:text="${commitInfo.messageShort}">
                commit-message #0.1
              </span>
            </div>
          </div>

          <div class="current-minor-groups" th:each="minorGroup: ${git.releaseCatalog.minorGroups}">
            <div class="minor-group">
              <div class="major-group-header">
                <span class="minor-group-date-time" th:text="${minorGroup.finalDateTimeStr()}">
                  current-minor-group-date-time
                </span>
                <span class="minor-group-version">
                  [(${ gbh.badgeJavaDocHTML(minorGroup) })]
                </span>
                <span class="minor-group-info">
                  [(${ gbh.badgeGitHubHTML(minorGroup) })]
                  [[${ minorGroup.groupInfo }]]
                </span>
              </div>
              <div class="minor-group-commits" th:each="commitInfo: ${minorGroup.commits}">
                <span class="commit-date-time" th:text="${commitInfo.localCommitTimeStr()}">
                  COMMIT-TIME-#1.1
                </span>
                <span class="commit-ID">
                  [(${ gbh.badgeCommit(commitInfo) })]
                </span>
                <span class="commit-message" th:text="${commitInfo.messageShort}">
                  commit-message #1.1
                </span>
                |<code>[(${git.releaseCatalog.linkedTriplet(commitInfo)})]</code>|
              </div>
            </div>
          </div>

          <div class="major-group" th:each="majorGroup: ${git.releaseCatalog.majorGroups}">
            <div class="major-group-header">
              <span class="major-group-date-time" th:text="${majorGroup.finalDateTimeStr()}">
                major-group-date-time
              </span>
              <span class="major-group-version">
                [(${ gbh.badgeJavaDocHTML(majorGroup) })]
              </span>
              <span class="major-group-info">
                [(${ gbh.badgeGitHubHTML(majorGroup) })]
                [[${ majorGroup.groupInfo }]]
              </span>
            </div>
            <div class="major-group-commits" th:each="commitInfo: ${majorGroup.currentMinor().commits}">
              <span class="commit-date-time" th:text="${commitInfo.localCommitTimeStr()}">
                COMMIT-TIME-#2.0.1
              </span>
              <span class="commit-ID">
                [(${ gbh.badgeCommit(commitInfo) })]
              </span>
                <span class="commit-message" th:text="${commitInfo.messageShort}">
                commit-message #2.0.1
              </span>
            </div>

            <div class="minor-groups" th:each="minorGroup: ${majorGroup.minorGroups}">
              <div class="minor-group">
                <div class="major-group-header">
                  <span class="minor-group-date-time" th:text="${minorGroup.finalDateTimeStr()}">
                    current-minor-group-date-time
                  </span>
                  <span class="minor-group-version">
                    [(${ gbh.badgeJavaDocHTML(minorGroup) })]
                  </span>
                  <span class="minor-group-info">
                    [(${ gbh.badgeGitHubHTML(minorGroup) })]
                    [[${ minorGroup.groupInfo }]]
                  </span>
                </div>
                <div class="minor-group-commits" th:each="commitInfo: ${minorGroup.commits}">
                  <span class="commit-date-time" th:text="${commitInfo.localCommitTimeStr()}">
                    COMMIT-TIME-#2.1.1
                  </span>
                  <span class="commit-ID">
                    [(${ gbh.badgeCommit(commitInfo) })]
                  </span>
                  <span class="commit-message" th:text="${commitInfo.messageShort}">
                    commit-message #2.1.1
                  </span>
                </div>
              </div>
            </div>
          </div>

        </div>

  </main>
