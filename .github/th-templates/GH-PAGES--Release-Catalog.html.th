<body class="package-index-page">

<header role="banner">
  <nav role="navigation">
    <!-- ========= START OF TOP NAVBAR ======= -->
    <div class="top-nav" id="navbar-top">
      <div class="nav-content">
        <h1 class="title">
          <span>[[${ mh.projectArtifact }]]</span>
          (<small>Release Catalog</small>)
          <a href="https://github.com/krm-demo/core-utils/">
            <img alt="an image to return back to the project-site"
                 src="resource-files/git-hub-logo--white-inv.svg"
                 width="32px" height="32px" />
          </a>
        </h1>
      </div>
    </div>
    <!-- ========= END OF TOP NAVBAR ========= -->
  </nav>
</header>

<div class="main-grid">
  <main role="main">

    <div class="header">
      <h2 class="title">Snapshot and Internal Releases:</h2>
    </div>

    <div class="release-catalog-section">

      <div class="major-grid-container">
        <div class="major-grid-title-row-over">
          <div class="col-1">
            [[${ git.releaseCatalog.snapshotYearAndMonth }]]
          </div>
          <div class="col-2">
            -[[${ git.releaseCatalog.snapshotDayOfMonthAndWeek }]]
          </div>
          <div class="col-3">
            [[${ git.releaseCatalog.snapshotHoursMinutes }]]
          </div>
          <div class="col-4">
            [(${ gbh.badgeSnapshotJavaDocHTML })]
          </div>
          <div class="col-5">
            [(${ gbh.badgeSnapshotGitHubHTML })]
            <span>[[${ git.releaseCatalog.snapshotGroup.groupInfo }]]</span>
          </div>
        </div>
        <details open>
          <summary>
            <div class="major-grid-title-row">
              <div class="col-1">
                [[${ git.releaseCatalog.snapshotYearAndMonth }]]
              </div>
              <div class="col-2">
                -[[${ git.releaseCatalog.snapshotDayOfMonthAndWeek }]]
              </div>
              <div class="col-3">
                [[${ git.releaseCatalog.snapshotHoursMinutes }]]
              </div>
              <div class="col-4">
                [(${ gbh.badgeSnapshotJavaDocHTML })]
              </div>
              <div class="col-5">
                [(${ gbh.badgeSnapshotGitHubHTML })]
                <span>[[${ git.releaseCatalog.snapshotGroup.groupInfo }]]</span>
              </div>
            </div>
          </summary>

          <div class="major-grid-commit-row" th:each="commitInfo: ${git.releaseCatalog.snapshotGroup.commits}">
            <div class="col-1">
              [[${ git.releaseCatalog.linkedTriplet(commitInfo).yearAndMonth }]]
            </div>
            <div class="col-2">
              -[[${ git.releaseCatalog.linkedTriplet(commitInfo).dayOfMonthAndWeek }]]
            </div>
            <div class="col-3">
              [[${ git.releaseCatalog.linkedTriplet(commitInfo).hoursMinutes }]]
            </div>
            <div class="col-4">
              [(${ gbh.badgeCommit(commitInfo) })]
            </div>
            <div class="col-5">
              [[${ commitInfo.messageShort }]]
            </div>
            <div class="col-6">...</div>
          </div>
        </details>
      </div>

      <!-- ------------------------------------------------------------------------------------ -->

      <div class="minor-grid-container" th:each="minorGroup: ${git.releaseCatalog.minorGroups}">
        <div class="minor-grid-title-row-over">
          <div class="col-1">
            [[${ git.releaseCatalog.linkedTriplet(minorGroup.finalCommit).yearAndMonth }]]
          </div>
          <div class="col-2">
            -[[${ git.releaseCatalog.linkedTriplet(minorGroup.finalCommit).dayOfMonthAndWeek }]]
          </div>
          <div class="col-3">
